  Protected Sub FVpurOrders_Init(ByVal sender As Object, ByVal e As System.EventArgs) Handles FVpurOrders.Init
    DataClassName = "EpurOrders"
    SetFormView = FVpurOrders
  End Sub
  Protected Sub TBLpurOrders_Init(ByVal sender As Object, ByVal e As System.EventArgs) Handles TBLpurOrders.Init
    SetToolBar = TBLpurOrders
  End Sub
  Protected Sub FVpurOrders_PreRender(ByVal sender As Object, ByVal e As System.EventArgs) Handles FVpurOrders.PreRender
    TBLpurOrders.EnableSave = Editable
    TBLpurOrders.EnableDelete = Deleteable
    TBLpurOrderDetails.EnableAdd = Editable
    TBLpurOrders.PrintUrl &= Request.QueryString("OrderNo") & "|"
    TBLpurOrders.PrintUrl &= Request.QueryString("OrderRev") & "|"
    Dim mStr As String = ""
    Dim oTR As IO.StreamReader = New IO.StreamReader(HttpContext.Current.Server.MapPath("~/PUR_Main/App_Edit") & "/EF_purOrders.js")
    mStr = oTR.ReadToEnd
    oTR.Close()
    oTR.Dispose()
    If Not Page.ClientScript.IsClientScriptBlockRegistered("scriptpurOrders") Then
      Page.ClientScript.RegisterClientScriptBlock(GetType(System.String), "scriptpurOrders", mStr)
    End If
  End Sub
  Partial Class gvBase
    Inherits SIS.SYS.GridBase
  End Class
  Private WithEvents gvpurOrderDetailsCC As New gvBase
  Protected Sub GVpurOrderDetails_Init(ByVal sender As Object, ByVal e As System.EventArgs) Handles GVpurOrderDetails.Init
    gvpurOrderDetailsCC.DataClassName = "GpurOrderDetails"
    gvpurOrderDetailsCC.SetGridView = GVpurOrderDetails
  End Sub
  Protected Sub TBLpurOrderDetails_Init(ByVal sender As Object, ByVal e As System.EventArgs) Handles TBLpurOrderDetails.Init
    gvpurOrderDetailsCC.SetToolBar = TBLpurOrderDetails
  End Sub
  Protected Sub GVpurOrderDetails_RowCommand(ByVal sender As Object, ByVal e As System.Web.UI.WebControls.GridViewCommandEventArgs) Handles GVpurOrderDetails.RowCommand
    If e.CommandName.ToLower = "lgedit".ToLower Then
      Try
        Dim OrderNo As Int32 = GVpurOrderDetails.DataKeys(e.CommandArgument).Values("OrderNo")  
        Dim OrderLine As Int32 = GVpurOrderDetails.DataKeys(e.CommandArgument).Values("OrderLine")  
        Dim OrderRev As Int32 = GVpurOrderDetails.DataKeys(e.CommandArgument).Values("OrderRev")  
        Dim RedirectUrl As String = TBLpurOrderDetails.EditUrl & "?OrderNo=" & OrderNo & "&OrderLine=" & OrderLine & "&OrderRev=" & OrderRev
        Response.Redirect(RedirectUrl)
      Catch ex As Exception
        ScriptManager.RegisterClientScriptBlock(Page, Page.GetType(), "", "alert('" & New JavaScriptSerializer().Serialize(ex.Message) & "');", True)
      End Try
    End If
    If e.CommandName.ToLower = "Deletewf".ToLower Then
      Try
        Dim OrderNo As Int32 = GVpurOrderDetails.DataKeys(e.CommandArgument).Values("OrderNo")  
        Dim OrderLine As Int32 = GVpurOrderDetails.DataKeys(e.CommandArgument).Values("OrderLine")  
        Dim OrderRev As Int32 = GVpurOrderDetails.DataKeys(e.CommandArgument).Values("OrderRev")  
        SIS.PUR.purOrderDetails.DeleteWF(OrderNo, OrderLine, OrderRev)
        GVpurOrderDetails.DataBind()
      Catch ex As Exception
        ScriptManager.RegisterClientScriptBlock(Page, Page.GetType(), "", "alert('" & New JavaScriptSerializer().Serialize(ex.Message) & "');", True)
      End Try
    End If
  End Sub
  Protected Sub TBLpurOrderDetails_AddClicked(ByVal sender As Object, ByVal e As System.Web.UI.ImageClickEventArgs) Handles TBLpurOrderDetails.AddClicked
    Dim OrderNo As Int32 = CType(FVpurOrders.FindControl("F_OrderNo"),TextBox).Text
    Dim OrderRev As Int32 = CType(FVpurOrders.FindControl("F_OrderRev"),TextBox).Text
    TBLpurOrderDetails.AddUrl &= "&OrderNo=" & OrderNo
    TBLpurOrderDetails.AddUrl &= "&OrderRev=" & OrderRev
  End Sub
